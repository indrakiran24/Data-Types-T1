<body>
    <h1 class="z">kiran</h1>
    <h1 class="x">phani</h1>
    <h1 class="v">jithya</h1>
    <script>
        let ab=document.querySelector('z')
        console.log(ab)
       for(let i=0;i<ab.lenght;i++){
            console.log(ab[i])
        } 
    </script>
</body>
</html>



<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1 class="z">kiran</h1>
    <h1 class="x">phani</h1>
    <h1 class="v">jithya</h1>
    <script>
      let ab = document.getElementsByTagName("h1");
      console.log(ab);
      for (let i = 0; i < ab.lenght; i++) {
        console.log(ab[i]);
      }
    </script>
    <script>
        for(let i=0;i<t.lenght;i++){
            let tt=document.createElement('h1')
            tt.className="aa"
            tt.text
        }
    </script>
  </body>
</html>
